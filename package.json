  "repository": {
    "type": "git",
    "url": "git+https://github.com/tormozz48/azure-pipelines-logger.git"
  },
  "keywords": [
    "logger",
    "cli",
    "azure",
    "azure-pipelines",
    "output"
  ],
  "author": "tormozz48@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tormozz48/azure-pipelines-logger/issues"
  },
  "homepage": "https://github.com/tormozz48/azure-pipelines-logger#readme",
  "pre-commit": [
    "build",
    "lint"
  ],
  "dependencies": {
    "consola": "^2.15.3",
    "dateformat": "^4.5.1"
  },
  "devDependencies": {
    "@types/consola": "^2.2.5",
    "@types/dateformat": "^3.0.1",
    "@types/node": "^14.14.31",
    "@types/sinon": "^9.0.11",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.21.0",
    "eslint-config-strict": "^14.0.1",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-jest": "^24.3.4",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-sinon": "^1.0.4",
    "pre-commit": "^1.2.2",
    "sinon": "^9.2.4",
    "ts-jest": "^26.5.2",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.2"
